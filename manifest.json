{
   "name": "GitHub Time Tracking for AC",
   "version": "0.1",
   "manifest_version": 2,

   "description": "Add time tracking to the GitHub Issue Tracker with AC.",
   "content_scripts": [ {
      "js": [ "javascripts/index.js" ],
      "css": [ "stylesheets/index.css"],
      "matches": [ "https://github.com/*/issues/*", "https://github.com/*/pull/*" ]
   } ],
   "permissions": [ "https://github.com/*", "http://localhost:3000/*" ],
   "web_accessible_resources": ["images/timer-icon.png"]
}
